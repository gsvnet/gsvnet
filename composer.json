{
	"name": "laravel/laravel",
	"description": "The Laravel Framework.",
	"keywords": ["framework", "laravel"],
	"license": "MIT",
	"type": "project",
	"require": {
		"laravel/framework": "~5.2.0",
		"laravelcollective/html": "5.2.*",
		"laravelcollective/bus": "5.2.*",
        "doctrine/dbal": "~2.3",
		"anahkiasen/former": "~4.0.0",
		"fzaninotto/faker": "~1.5.0",
		"intervention/image": "~2.3.0",
	    "chromabits/purifier": "~2.1",
		"nickcernis/html-to-markdown": "~2.1.0",
		"misd/linkify": "~1.1.2",
		"guzzlehttp/guzzle": "~4.0",
		"laracasts/presenter": "~0.2.1",
		"pda/pheanstalk": "~3.0",
		"msurguy/honeypot": "~1.0.1",
		"mailchimp/mailchimp": "~2.0.4",
		"erusev/parsedown": "~1.6.0",
        "haampie/gravatar": "~1.4.0",
		"predis/predis": "~1.0",
		"laracasts/flash": "~1.3"
	},
	"require-dev": {
		"phpunit/phpunit": "~4.0",
		"phpspec/phpspec": "~2.1",
		"symfony/dom-crawler": "~3.0",
		"symfony/css-selector": "~3.0"
	},
	"repositories": [
		{
			"type": "vcs",
			"url": "git@github.com:haampie/gravatar.git"
		}
	],
	"autoload": {
		"classmap": [
		    "app/Http/Controllers",
		    "app/Console/Commands",
			"database"
		],
		"psr-4": {
			"GSV\\": "app/",
		    "GSVnet\\": "GSVnet/"
		}
	},
	"autoload-dev": {
		"classmap": [
			"tests/TestCase.php"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
		    "php artisan optimize"
		],
		"post-create-project-cmd": [
			"php -r \"copy('.env.example', '.env');\"",
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	}
}

{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "laravel/framework": "5.4.*",
        "laravelcollective/html": "5.4.*",
        "laravelcollective/bus": "5.2.*",
        "doctrine/dbal": "~2.3",
        "anahkiasen/former": "~4.0.0",
        "fzaninotto/faker": "~1.5.0",
        "intervention/image": "~2.3.0",
        "chromabits/purifier": "~2.1",
        "misd/linkify": "~1.1.2",
        "guzzlehttp/guzzle": "~6.0",
        "laracasts/presenter": "~0.2.1",
        "pda/pheanstalk": "~3.0",
        "msurguy/honeypot": "~1.0.1",
        "mailchimp/mailchimp": "~2.0.4",
        "erusev/parsedown": "~1.7.2",
        "haampie/gravatar": "~1.4.0",
        "predis/predis": "~1.0",
        "laracasts/flash": "~1.3",
        "league/csv": "^8.0",
        "league/fractal": "^0.13.0",
        "barryvdh/laravel-cors": "^0.8.0",
        "maatwebsite/excel": "~2.1.0",
        "giggsey/libphonenumber-for-php": "^7.4",
        "bugsnag/bugsnag-laravel": "^1.6",
        "algolia/algoliasearch-client-php": "^1.10",
        "php": ">=5.6.4",
        "laravel/tinker": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7",
        "phpspec/phpspec": "~2.1"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:haampie/gravatar.git"
        }
    ],
    "autoload": {
        "classmap": [
            "app/Http/Controllers",
            "app/Console/Commands",
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "post-create-project-cmd": [
            "php -r \"copy('.env.example', '.env');\"",
            "php artisan key:generate"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}

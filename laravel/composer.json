{
	"name": "laravel/laravel",
	"description": "The Laravel Framework.",
	"keywords": ["framework", "laravel"],
	"require": {
		"laravel/framework": "4.2.*",
		"way/generators": "2.*",
		"anahkiasen/former": "dev-master",
		"fzaninotto/faker": "1.2.*@dev",
		"intervention/image": "2.*",
		"thomaswelton/laravel-gravatar": "dev-master",
        "michelf/php-markdown": "1.4.*@dev",
        "mews/purifier": "dev-master",
        "nickcernis/html-to-markdown": "dev-master",
        "misd/linkify": "1.1.*@dev",
        "fedeisas/laravel-mail-css-inliner": "1.*",
        "guzzlehttp/guzzle": "~4.0",
        "laracasts/presenter": "0.1.*",
		"pda/pheanstalk": "dev-master",
		"msurguy/honeypot": "dev-master"
	},
	"autoload": {
		"classmap": [
			"app/commands",
			"app/controllers",
			"app/database/migrations",
			"app/database/seeds",
			"app/tests/TestCase.php"
		],
		"psr-4": {
			"GSVnet\\": "app/GSVnet/"
		}
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan optimize"
		],
		"pre-update-cmd": [
			"php artisan clear-compiled"
		],
		"post-update-cmd": [
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "dev"
}
